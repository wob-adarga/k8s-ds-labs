{
  "pipeline": {
    "name": "svo-extraction"
  },
  "description": "Subject-Verb-Object extraction based on noun chunks.",
  "transform": {
    "cmd": [ "python3", "-m", "service", "svo-extraction"],
    "image": "eu.gcr.io/adarga-docker/ds-knowledge-store-transform:1.3.8",
    "env": {
      "SERVICE_URL_BASE": "http://svoextraction-default.seldon.svc.cluster.local:8000",
      "SERVICE_ENDPOINT": "api/v1.0/predictions"
    }
  },
  "input": {
    "pfs": {
      "repo": "sentence-extraction",
      "glob": "/*/*"
    }
  },
  "enable_stats": false,
  "egress": {
    "URL": "s3://adarga-ds-lab-1-pachyderm-egress/svo-extraction"
  }
}
