{
  "pipeline": {
    "name": "event-grouper"
  },
  "description": "Event grouping.",
  "transform": {
    "cmd": [ "python3", "-m", "service", "event-grouper"],
    "image": "eu.gcr.io/adarga-docker/ds-knowledge-store-transform:1.17.0",
    "env": {
      "SERVICE_URL_BASE": "http://event-grouper1-17-0-default.seldon.svc.cluster.local:8000",
      "SERVICE_ENDPOINT": "api/v1.0/predictions"
    }
  },
  "input": {
    "pfs": {
      "repo": "event-detector",
      "glob": "/*/*"
    }
  },
  "enable_stats": false,
  "egress": {
    "URL": "s3://adarga-ds-lab-1-pachyderm-egress/event-grouper"
  }
}
