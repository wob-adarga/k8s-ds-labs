{
  "pipeline": {
    "name": "mention-extraction"
  },
  "description": "Mention extraction based on noun chunks.",
  "transform": {
    "cmd": [ "python3", "-m", "service", "mention-extraction"],
    "image": "eu.gcr.io/adarga-docker/ds-knowledge-store-transform:1.3.7",
    "env": {
      "SERVICE_URL": "http://mentionextraction-default.seldon.svc.cluster.local:8000",
      "SERVICE_ENDPOINT": "api/v1.0/predictions"
    }
  },
  "input": {
    "pfs": {
      "repo": "sentence-extraction",
      "glob": "/*/*",
      "branch": "master"
    }
  },
  "enable_stats": false,
  "egress": {
    "URL": "s3://adarga-ds-lab-1-pachyderm-egress/feed-mention-extraction"
  }
}
