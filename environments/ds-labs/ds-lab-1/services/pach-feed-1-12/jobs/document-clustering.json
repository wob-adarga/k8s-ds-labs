{
  "pipeline": {
    "name": "document-clustering"
  },
  "metadata": {
    "annotations": {
        "document-clustering-model-version": "0.0.0"
    }
  },
  "description": "document clustering pipeline",
  "transform": {
    "cmd": [ "python3", "DocumentClusteringPipeline.py"],
    "image": "eu.gcr.io/adarga-docker/document-clustering-pipeline:0.0.0",
    "env": {
      "SERVICE_URL_BASE": "http://document-clustering-default.seldon.svc.cl√üuster.local:8000",
      "SERVICE_ENDPOINT": "api/v1.0/predictions",
      "DOCUMENTS_REPO": "/pfs/clean-ingress-feed",
      "MENTIONS_REPO": "/pfs/mention-typer",
      "DOC2VEC_REPO": "/pfs/doc2vec",
      "OUTPUT_REPO": "/pfs/out"
    }
  },
  "input": {
    "join": [
        {
        "repo": "clean-ingress-feed",
        "glob": "/*/*",
        "branch": "master"
      },
      {
        "repo": "doc2vec",
        "glob": "/*/*",
        "branch": "master"
      },
      {
        "repo": "mention-typer",
        "glob": "/*/*",
        "branch": "master"
      }
    ]
  },
  "enable_stats": false,
  "egress": {
    "URL": "s3://adarga-ds-lab-1-pachyderm-egress/feed-document-clustering"
  }
}