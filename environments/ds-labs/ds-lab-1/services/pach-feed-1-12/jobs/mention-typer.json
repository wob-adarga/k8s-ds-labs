{
  "pipeline": {
    "name": "mention-typer"
  },
  "description": "Mention typing.",
  "resource_requests": {
    "cpu": 1,
    "memory": "1G"
  },
  "transform": {
    "cmd": [ "python3", "-m", "service", "mention-typer" ],
    "image": "eu.gcr.io/adarga-docker/ds-knowledge-store-transform:1.2.0",
    "env": {
      "SERVICE_URL": "http://mention-typer-default-mention-typer.seldon.svc.cluster.local:9000/api/v1.0/predictions"
    }
  },
  "input": {
    "pfs": {
      "repo": "sentence-extraction",
      "glob": "/*/*",
      "branch": "master"
    }
  },
  "enable_stats": false,
  "egress": {
    "URL": "s3://adarga-ds-lab-1-pachyderm-egress/feed-mention-typer"
  }
}
