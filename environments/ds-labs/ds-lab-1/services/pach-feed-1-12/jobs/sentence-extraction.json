{
  "pipeline": {
    "name": "sentence-extraction"
  },
  "description": "Sentence extraction based on NLTK punkt.",
  "resource_requests": {
    "cpu": 1,
    "memory": "1G"
  },
  "transform": {
    "cmd": [ "python3", "-m", "service", "sentence-extraction" ],
    "image": "eu.gcr.io/adarga-docker/ds-knowledge-store-transform:1.8.0",
    "env": {
      "SERVICE_URL_BASE": "http://sentence-extraction-default.seldon.svc.cluster.local:8000",
      "SERVICE_ENDPOINT": "api/v1.0/predictions"
    }
  },
  "input": {
    "pfs": {
      "repo": "clean-ingress-feed",
      "glob": "/*/*",
      "branch": "master"
    }
  },
  "enable_stats": false,
  "egress": {
    "URL": "s3://adarga-ds-lab-1-pachyderm-egress/feed-sentence-extraction"
  }
}
