{
    "pipeline": {
      "name": "event-detector"
    },
    "description": "Event typer based on lookup table.",
    "transform": {
      "cmd": [ "python3", "-m", "service", "event-detector"],
      "image": "eu.gcr.io/adarga-docker/ds-knowledge-store-transform:1.2.0",
      "env": {
        "SERVICE_URL": "http://event-detector-default-event-detector.seldon.svc.cluster.local:9000/api/v1.0/predictions"
      }
    },
    "input": {
      "pfs": {
        "repo": "sentence-extraction",
        "glob": "/*/*",
        "branch": "master"
      }
    },
    "enable_stats": false,
    "egress": {
      "URL": "s3://adarga-ds-lab-1-pachyderm-egress/feed-event-detector"
    }
  }
  